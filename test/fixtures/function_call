{
    "simple": myfunc("a1", null, "a2"),
}

--
$ canif -i 4
{
    "simple": myfunc(
        "a1",
        null,
        "a2",
    ),
}

--
$ canif -i 4 -j
{
    "simple": {
        "$$myfunc": [
            "a1",
            null,
            "a2"
        ]
    }
}

--
$ canif -i 4 --no-trailing-commas
{
    "simple": myfunc(
        "a1",
        null,
        "a2"
    )
}
