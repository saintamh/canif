{
    "one": {1},
    "one+trail": {1,},
    "two": {1, 2},
    "two+trail": {1, 2,},
    "nested": {1, {2, {3}}},
    "nested+trail": {1, {2, {3},},},
}

$ canif -i 4
{
    "one": {
        1,
    },
    "one+trail": {
        1,
    },
    "two": {
        1,
        2,
    },
    "two+trail": {
        1,
        2,
    },
    "nested": {
        1,
        {
            2,
            {
                3,
            },
        },
    },
    "nested+trail": {
        1,
        {
            2,
            {
                3,
            },
        },
    },
}

$ canif -i 4 -j
{
    "one": {
        "$set": [
            1,
        ],
    },
    "one+trail": {
        "$set": [
            1,
        ],
    },
    "two": {
        "$set": [
            1,
            2,
        ],
    },
    "two+trail": {
        "$set": [
            1,
            2,
        ],
    },
    "nested": {
        "$set": [
            1,
            {
                "$set": [
                    2,
                    {
                        "$set": [
                            3,
                        ],
                    },
                ],
            },
        ],
    },
    "nested+trail": {
        "$set": [
            1,
            {
                "$set": [
                    2,
                    {
                        "$set": [
                            3,
                        ],
                    },
                ],
            },
        ],
    },
}
