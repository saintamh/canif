{
    "empty": {},
    "one": {"a": 1},
    "two": {"a": 1, "b": 2},
    "naked_keys": {a: 1},
    "array": {a: [1, 2]},
    "tuple": {a: (1, 2)},
    "set": {a: {1, 2}},
    "nested": {a: {b: 2}},
    "list_as_key": {[1, 2]: 3},
    "tuple_as_key": {(1, 2): 3},
}

--
$ canif -i 4
{
    "empty": {},
    "one": {
        "a": 1,
    },
    "two": {
        "a": 1,
        "b": 2,
    },
    "naked_keys": {
        a: 1,
    },
    "array": {
        a: [
            1,
            2,
        ],
    },
    "tuple": {
        a: (
            1,
            2,
        ),
    },
    "set": {
        a: {
            1,
            2,
        },
    },
    "nested": {
        a: {
            b: 2,
        },
    },
    "list_as_key": {
        [
            1,
            2,
        ]: 3,
    },
    "tuple_as_key": {
        (
            1,
            2,
        ): 3,
    },
}

--
$ canif -i 4 -j
{
    "empty": {},
    "one": {
        "a": 1
    },
    "two": {
        "a": 1,
        "b": 2
    },
    "naked_keys": {
        "a": 1
    },
    "array": {
        "a": [
            1,
            2
        ]
    },
    "tuple": {
        "a": [
            1,
            2
        ]
    },
    "set": {
        "a": {
            "$set": [
                1,
                2
            ]
        }
    },
    "nested": {
        "a": {
            "b": 2
        }
    },
    "list_as_key": {
        [
            1,
            2
        ]: 3
    },
    "tuple_as_key": {
        [
            1,
            2
        ]: 3
    }
}
